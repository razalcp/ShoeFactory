    <script>

        const statusElements = document.getElementsByClassName('status');

        Array.from(statusElements).forEach(statusElement => {

            statusElement.addEventListener('change', async function (event) {
                const orderId = statusElement.dataset.orderid
                const productId = statusElement.dataset.productid


                const selectedOption = event.target.value;


                console.log('Selected option:', selectedOption);
                console.log('orderID: ', orderId);
                console.log('productID: ', productId);

                let url = `http://localhost:3003/updateOrderStatus?status=${selectedOption}&productId=${productId}&orderId=${orderId}`

                const result = await fetch(url)
                console.log(result);
                if (result) {
                    window.location.reload()
                }
                var data = await result.json()

                console.log(data.message);



            });
        });


    </script>
