    <div class="contact__area">
        <div class="container">
            <div class="row">
                <div class="col-xl-12 col-lg-12 col-md-12">
                    <div class="contact__content text-center mb-140">
                        <h2>Order Detail Area</h2>
                        <p>Gratitude unlocks the fullness of life. It turns what we have into enough, and more. It turns
                            denial into acceptance, chaos to order, confusion to clarity. <br> It can turn a meal into a
                            feast, a house into a home, a stranger into a friend. </p>
                    </div>
                </div>
            </div>
            <div class="contact-wrapper">
                <div class="row">
                    <div class="col-xl-6 col-lg-6 col-md-12 col-sm-6" style="width: 1240px;">
                        <div class="contactbox">
                            <div class="contactbox__heading">
                                <!-- <h5>Contact us</h5>
                            <h2>Please contact us quickly if <br>you need help.</h2> -->
                            </div>

                            <h1 style="margin-left: 513px;text-decoration: underline;color:dimgray">Order details</h1>
                            <% if(locals.orderData) { %>
                                <% orderData.products.forEach(ele=> { %>
                                    <%if(ele.status==='Delivered'){%>
                                        <a href="/invoice?orderId=<%=orderData.id%>"><button onclick=""
                                                style="margin-bottom: 25px;margin-left: 1000px;">View
                                                invoice</button></a>
                                        <% }%>

                                            <% }) %>
                                                <% }%>
                                                    <div class="row">

                                                        <div class="col-sm-6">
                                                            <div class="card">
                                                                <div class="card-body">
                                                                    <h4 class="card-title"
                                                                        style="margin-left: 190px;text-decoration: underline;">
                                                                        Delivery Address</h4>
                                                                    <% if(locals.orderData) { %>
                                                                        <% orderData.address.forEach(el=> { %>
                                                                            <h4 class="card-text ">Name : <%=el.uname%>
                                                                            </h4>
                                                                            <h4 class="card-text ">Mobile: <%=el.mobile
                                                                                    %>
                                                                            </h4>
                                                                            <h4 class="card-text ">Flat No or Name:
                                                                                <%=el.address%><br></h4>

                                                                            <h4 class="" card-text>City: <%=el.city %>
                                                                            </h4>
                                                                            <h4 class="card-text">Pincode:
                                                                                <%=el.pincode%>
                                                                            </h4>
                                                                            <h4 class="card-text">State: <%=el.state%>
                                                                            </h4>
                                                                            <h4 class="card-text">Country: India</h4>


                                                                            <% }) %>
                                                                                <% } %>
                                                                                    <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="card">
                                                                <div class="card-body">
                                                                    <h4 class="card-title"
                                                                        style="margin-left: 190px;text-decoration: underline;">
                                                                        Product Details</h4>
                                                                    <% if(locals.orderData){ %>
                                                                        <h4 class="card-text">Order Id :
                                                                            <%=orderData.orderId %>
                                                                        </h4>
                                                                        <% orderData.products.forEach(ele=> { %>
                                                                            <h4 class="card-text">Product :
                                                                                <%=ele.product_id.producttitle %> x
                                                                                    <%=ele.qty%> <img
                                                                                            style="height:50px;width:38px"
                                                                                            src="/public/images/<%=ele.product_id.imageurl[1]%>"
                                                                                            alt=""></h4>
                                                                            <h4 class="card-text">Payment Methord :
                                                                                <%=orderData.paymentMethord%>
                                                                            </h4>
                                                                            <h4 class="card-text">Total Price : &#8377;
                                                                                <%=orderData.totalPrice %>
                                                                            </h4>



                                                                            <% if(ele.status==="Processing" ) { %>
                                                                                <h4>Status: <span
                                                                                        class="badge rounded-pill alert-warning getStatus">Processing</span>
                                                                                </h4>

                                                                                <% } else
                                                                                    if(ele.status==="Cancel Requested" )
                                                                                    {%>
                                                                                    <h4>Status: <span
                                                                                            class="badge rounded-pill alert-danger getStatus">Cancel
                                                                                            Requested</span>
                                                                                    </h4>
                                                                                    <% } else
                                                                                        if(ele.status==="Cancel Accepted"
                                                                                        ) {%>
                                                                                        <h4>Status: <span
                                                                                                class="badge rounded-pill alert-success getStatus">Cancel
                                                                                                Accepted</span>
                                                                                        </h4>
                                                                                        <% } else
                                                                                            if(ele.status==="Cancel Declined"
                                                                                            ) {%>
                                                                                            <h4>Status: <span
                                                                                                    class="badge rounded-pill alert-success getStatus">Cancel
                                                                                                    Declined</span>
                                                                                            </h4>
                                                                                            <% } else
                                                                                                if(ele.status==="Delivered"
                                                                                                ){ %>
                                                                                                <h4>Status: <span
                                                                                                        class="badge rounded-pill alert-success getStatus">Delivered</span>
                                                                                                </h4>
                                                                                                <% } else
                                                                                                    if(ele.status==="Shipped"
                                                                                                    ) {%>
                                                                                                    <h4>Status: <span
                                                                                                            class="badge rounded-pill alert-info">Shipped</span>
                                                                                                    </h4>
                                                                                                    <% } else
                                                                                                        if(ele.status==="Dispatched"
                                                                                                        ){ %>
                                                                                                        <h4>Status:
                                                                                                            <span
                                                                                                                class="badge rounded-pill alert-info getStatus">Dispatched</span>
                                                                                                        </h4>
                                                                                                        <% }else
                                                                                                            if(ele.status==="Return Requested"
                                                                                                            ){ %>
                                                                                                            <h4>Status:
                                                                                                                <span
                                                                                                                    class="badge rounded-pill bg-dark text-light alert-dark getStatus">Return
                                                                                                                    Requested</span>
                                                                                                            </h4>
                                                                                                            <% }else
                                                                                                                if(ele.status==="Return Accepted"
                                                                                                                ){ %>
                                                                                                                <h4>Status:
                                                                                                                    <span
                                                                                                                        class="badge rounded-pill  alert-dark getStatus">Return
                                                                                                                        Accepted</span>
                                                                                                                </h4>
                                                                                                                <% } else
                                                                                                                    if(ele.status==="Return Declined"
                                                                                                                    )
                                                                                                                    {%>
                                                                                                                    <h4>Status:
                                                                                                                        <span
                                                                                                                            class="badge rounded-pill alert-danger getStatus">Return
                                                                                                                            Declined</span>
                                                                                                                    </h4>
                                                                                                                    <% }
                                                                                                                        %>
                                                                                                                        <h4>Order
                                                                                                                            Date
                                                                                                                            :
                                                                                                                            <%=orderData.createdAt.toISOString().split('T')[0]%>
                                                                                                                        </h4>
                                                                                                                        <% })
                                                                                                                            %>

                                                                                                                            <% }
                                                                                                                                %>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>







