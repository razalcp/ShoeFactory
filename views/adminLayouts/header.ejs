  box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.2);
  padding: 30px;
  z-index: 9999;
}

.popup-content {
  text-align: center;
}

.close {
  position: absolute;
  top: 10px;
  right: 10px;
  color: #ff0000;
  font-size: 24px;
  cursor: pointer;
}

